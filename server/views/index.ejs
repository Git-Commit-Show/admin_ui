    $(document).ready(function(){
      
      $.get("/links", function(data, status){
        for(i=0;i<data.length;i++)
        {
            var opt = document.createElement('option');
            opt.appendChild( document.createTextNode(data[i]));
            opt.value = data[i];
            opt.classList.add("dropdown-item","bg-gray-dark");
            live_link.insertBefore(opt,live_link.children[live_link.options.length-1]);
        }
   });
   $.get("/attendee_links", function(data, status){
    let live_link1 = document.getElementById('live_link1');
        for(i=0;i<data.length;i++)
        {
            var opt = document.createElement('option');
            opt.appendChild( document.createTextNode(data[i]));
            opt.value = data[i];
            opt.classList.add("dropdown-item","bg-gray-dark");
            live_link1.insertBefore(opt,live_link1.children[live_link1.options.length-1]);
        }
   });
});

    </script>
    <script>
        function change_status(){
        var status = document.getElementById('streaming_status').checked;
        if(status==true)
        {
          location.href = "/start";
        }
        else{
          location.href = "/stop";
        }
      }
      function switch_scenes(value){
        var x = document.getElementById("switch_scene").value;
       alert(x);
        location.href="/scenes/"+ x;       
       }
       function change_link(){
         var y = document.getElementById("live_link").value;
         if(y=="add_link"){
          var person = prompt("Please enter new live streaming link");
          if (person != null) {
            
            let live_link = document.getElementById('live_link');
            window.location.href='/add_link?link='+person+'&count='+live_link.options.length;
            
            alert("New Link added!");
}
         }
       }
       function change_attendee_link(){
         var y = document.getElementById("live_link1").value;
         if(y=="add_link"){
          var person = prompt("Please enter new attendee link");
          if (person != null) {
            let live_link = document.getElementById('live_link1');
            window.location.href='/add_attendee_link?link='+person+'&count='+live_link.options.length;
            alert("New Link added!");
}
         }
       }
    </script>
